.container
  .panel-magin
    div(ng-repeat='room in rooms')
      .col-md-4
        a(href='/room/{{room.id}}')
          .jumbotron.room-panel
            h2 {{room.idToUpper}}
            .room-tag {{room.tag.word}}
            | {{room.startedAt}} - {{room.endedAt}}
            p.help-block {{room.theme}}
